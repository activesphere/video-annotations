{
	"name": "va-inject-to-yt",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "Capture video frame from youtube iframe, and send to video-annotations app",
	"homepage_url": "http://example.com",
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},
	"browser_action": {
		"default_title": "VA Inject into Iframe"
	},
	"permissions": ["https://*/*", "http://*/*", "tabs"],

	"icons": {
		"48": "va.png"
	}
}
